<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="opencarshop.servico.controller.ServicoController">
    <children>
        <AnchorPane maxHeight="383.0" maxWidth="1000.0" minHeight="200.0" minWidth="350.0" prefHeight="383.0" prefWidth="457.0">
            <children>
                <TextField fx:id="tfdescricao" alignment="TOP_LEFT" depthTest="ENABLE" layoutX="14.0" layoutY="31.0" nodeOrientation="LEFT_TO_RIGHT" promptText="Descrição do serviço">
                    <tooltip>
                        <Tooltip text="User Name will need to Login" />
                    </tooltip>
                    <font>
                        <Font size="11.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="DEFAULT" />
                    </cursor>
                </TextField>
                <TextField fx:id="tfvalor" layoutX="165.0" layoutY="31.0" promptText="ex: 50,00">
                    <font>
                        <Font size="11.0" />
                    </font>
                </TextField>
                <Button fx:id="btnSignUp" layoutX="316.0" layoutY="74.0" mnemonicParsing="false" onAction="#cadastraServico" text="Cadastrar" />
                <Label layoutX="14.0" layoutY="14.0" text="Desccrição:" />
                <Label layoutX="165.0" layoutY="14.0" text="Valor:" />
                <CheckBox fx:id="chkValue" layoutX="312.0" layoutY="35.0" mnemonicParsing="false" text="Valor Fixo" />
                <Label fx:id="labelErroServ" layoutX="106.0" layoutY="176.0" textFill="#ee0303">
                    <font>
                        <Font size="11.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
    </children>
    
    
</AnchorPane>
